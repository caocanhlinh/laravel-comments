{
    "name": "caocanhlinh/laravel-comments",
    "description": "Comments for Laravel.",
    "keywords": [
        "laravel",
        "comments"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "erusev/parsedown": "^1.7",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0",
